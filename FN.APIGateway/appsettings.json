{
  "ReverseProxy": {
    "Routes": {
      "userRoute": {
        "ClusterId": "userCluster",
        "Match": {
          "Path": "api/user/{**catch-all}"
        }
      },
      "emailRoute": {
        "ClusterId": "emailCluster",
        "Match": {
          "Path": "api/email/{**catch-all}"
        }
      }
    }
  },
  "Clusters": {
    "userCluster": {
      "Destinations": {
        "userService": {
          "Address": "http://localhost:5001/"
        }
      }
    },
    "emailCluster": {
      "Destinations": {
        "emailService": {
          "Address": "http://localhost:5002/"
        }
      }
    }
  }
}
